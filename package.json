{
  "name": "check",
  "version": "0.0.0-development",
  "description": "GitHub Action to check for unowned dependencies using dependency-owners",
  "author": "Kirk Eaton <contact@kirkeaton.ca>",
  "bugs": {
    "url": "https://github.com/dependency-owners/check/issues"
  },
  "dependencies": {
    "@actions/core": "2.0.1",
    "dependency-owners": "3.0.2"
  },
  "devDependencies": {
    "@dependency-owners/package-json-loader": "2.0.0",
    "@kirkeaton/prettier-config": "1.0.6",
    "@kirkeaton/semantic-release-config": "2.0.1",
    "@kirkeaton/tsconfig": "2.1.1",
    "@types/node": "24.10.4",
    "esbuild": "0.27.2",
    "prettier": "3.7.4",
    "semantic-release": "25.0.2",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=24"
  },
  "exports": "./dist/index.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/dependency-owners/check#readme",
  "keywords": [
    "actions",
    "react",
    "setup"
  ],
  "license": "BSD-3-Clause",
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dependency-owners/check.git"
  },
  "scripts": {
    "build": "pnpm clean && ./build.mjs",
    "clean": "rm -rf dist",
    "format": "prettier \"**/*.{js,json,md,mjs,mts}\" --write",
    "lint": "prettier \"**/*.{js,json,md,mjs,mts}\" --check",
    "typecheck": "tsc --noEmit"
  },
  "type": "commonjs"
}
