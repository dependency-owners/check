{
  "name": "check",
  "version": "0.0.0-development",
  "description": "GitHub Action to check for unowned dependencies using dependency-owners ",
  "author": "Kirk Eaton <contact@kirkeaton.ca>",
  "bugs": {
    "url": "https://github.com/dependency-owners/check/issues"
  },
  "dependencies": {
    "@actions/core": "1.11.1",
    "dependency-owners": "1.1.1"
  },
  "devDependencies": {
    "@kirkeaton/prettier-config": "1.0.5",
    "@kirkeaton/semantic-release-config": "1.0.4",
    "@kirkeaton/tsconfig": "1.0.3",
    "@types/node": "22.17.2",
    "esbuild": "0.25.9",
    "prettier": "3.6.2",
    "semantic-release": "24.2.7",
    "typescript": "5.9.2"
  },
  "exports": "./dist/index.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/dependency-owners/check#readme",
  "keywords": [
    "actions",
    "react",
    "setup"
  ],
  "license": "BSD-3-Clause",
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dependency-owners/check.git"
  },
  "scripts": {
    "build": "pnpm clean && ./build.mjs",
    "clean": "rm -rf dist",
    "format": "prettier \"**/*.{js,json,md,mjs,mts}\" --write",
    "lint": "prettier \"**/*.{js,json,md,mjs,mts}\" --check",
    "typecheck": "tsc --noEmit"
  },
  "type": "commonjs"
}
