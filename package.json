{
  "name": "check",
  "version": "0.0.0-development",
  "description": "GitHub Action to check for unowned dependencies using dependency-owners",
  "author": "Kirk Eaton <contact@kirkeaton.ca>",
  "bugs": {
    "url": "https://github.com/dependency-owners/check/issues"
  },
  "dependencies": {
    "@actions/core": "1.11.1",
    "dependency-owners": "2.1.0"
  },
  "devDependencies": {
    "@kirkeaton/prettier-config": "1.0.5",
    "@kirkeaton/semantic-release-config": "1.1.0",
    "@kirkeaton/tsconfig": "2.1.0",
    "@types/node": "22.18.0",
    "esbuild": "0.25.9",
    "prettier": "3.6.2",
    "semantic-release": "24.2.7",
    "typescript": "5.9.2"
  },
  "exports": "./dist/index.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/dependency-owners/check#readme",
  "keywords": [
    "actions",
    "react",
    "setup"
  ],
  "license": "BSD-3-Clause",
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dependency-owners/check.git"
  },
  "scripts": {
    "build": "pnpm clean && ./build.mjs",
    "clean": "rm -rf dist",
    "format": "prettier \"**/*.{js,json,md,mjs,mts}\" --write",
    "lint": "prettier \"**/*.{js,json,md,mjs,mts}\" --check",
    "typecheck": "tsc --noEmit"
  },
  "type": "commonjs"
}
