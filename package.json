{
  "name": "check",
  "version": "0.0.0-development",
  "description": "GitHub Action to check for unowned dependencies using dependency-owners",
  "author": "Kirk Eaton <contact@kirkeaton.ca>",
  "bugs": {
    "url": "https://github.com/dependency-owners/check/issues"
  },
  "dependencies": {
    "@actions/core": "1.11.1",
    "dependency-owners": "3.0.1"
  },
  "devDependencies": {
    "@dependency-owners/package-json-loader": "2.0.0",
    "@kirkeaton/prettier-config": "1.0.5",
    "@kirkeaton/semantic-release-config": "1.1.0",
    "@kirkeaton/tsconfig": "2.1.0",
    "@types/node": "24.9.1",
    "esbuild": "0.25.11",
    "prettier": "3.6.2",
    "semantic-release": "25.0.1",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=24"
  },
  "exports": "./dist/index.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/dependency-owners/check#readme",
  "keywords": [
    "actions",
    "react",
    "setup"
  ],
  "license": "BSD-3-Clause",
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dependency-owners/check.git"
  },
  "scripts": {
    "build": "pnpm clean && ./build.mjs",
    "clean": "rm -rf dist",
    "format": "prettier \"**/*.{js,json,md,mjs,mts}\" --write",
    "lint": "prettier \"**/*.{js,json,md,mjs,mts}\" --check",
    "typecheck": "tsc --noEmit"
  },
  "type": "commonjs"
}
